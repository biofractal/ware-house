.container
	h2 {{house.client}}
	table.table.table-bordered
		tr.success
			th ID
			th Client
			th Products
		tr
			td {{house._id}}
			td {{house.client}}
			td
				table.table.table-bordered
					tr.info
						th Name
						th Price
						th Stock
					tr(ng-repeat="product in house.products")
						td {{product.name}}
						td {{product.price}}
						td {{product.stock}}





