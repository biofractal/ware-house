{
  "name": "ware-house",
  "version": "1.0.0",
  "description": "A central warehouse API for product management and pick-packing control",
  "main": "node-server.js",
  "scripts": {
    "start": "npm run clean && npm run transpile && npm run copy && npm run start-node",
    "clean": "if exist lib rmdir lib /s /q",
    "transpile": "start /B coffee --output ./lib -cw src",
    "copy": "copy .env lib",
    "start-node": "start /B nodemon ./lib/node-server.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/biofractal/ware-house.git"
  },
  "author": "biofractal <biofractal@gmail.com> (http://biofractal.blogspot.co.uk/)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/biofractal/ware-house/issues"
  },
  "homepage": "https://github.com/biofractal/ware-house",
  "dependencies": {
    "body-parser": "^1.12.0",
    "dotenv": "^0.5.1",
    "express": "^4.11.2",
    "mongoose": "^3.8.23",
    "request": "^2.53.0",
    "watch": "^0.14.0",
    "winston": "^0.9.0"
  },
  "devDependencies": {
    "npm-watch": "0.0.0"
  }
}
